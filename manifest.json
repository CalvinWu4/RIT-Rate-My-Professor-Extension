{
	"name" : "RIT Rate My Professors Extension",
	"version" : "1.2.1",
	"description" : "Shows the Rate My Professors rating of RIT professors while searching for classes on Tiger Center and CSH Schedule Maker.",
	"permissions": ["https://search-production.ratemyprofessors.com/*", "https://raw.githubusercontent.com/carltonnorthern/nickname-and-diminutive-names-lookup/master/names.csv/"],
	"content_scripts": [ {
		"css": [ "tooltipster/dist/css/tooltipster.main.min.css", "tooltip.css" ],
		"js": [ "jquery-3.3.1.min.js", "addedNicknames.js", "arrive.min.js", "tooltipster/dist/js/tooltipster.bundle.min.js", "contentscript.js" ],
		"matches": [ "https://tigercenter.rit.edu/tigerCenterApp/api/class-search*", "https://schedulemaker.csh.rit.edu/*" ]
	  }],
	"background": {
		"scripts": ["node_modules/papaparse/papaparse.min.js", "background.js"],
		"persistent": false
		},
		"icons": { "16": "images/icon16.png",
			"48": "images/icon48.png",
		 "128": "images/icon128.png" },
	"manifest_version" : 2
}